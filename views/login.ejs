<%- include('partials/header') %>


<div>
  <div align="Center">
    <img src="https://www.carboncents.com/logo-1.png" width="350" height="250"/>
  </div>
  
  <form action="/login" method="post">
    <div class="border border-5 shadow bg-white rounded" style="margin: 3% 32% 5% 32%; padding: 5% 5%">
      <div class="form-group my-3">
        <label for="email">Email</label>
        <input type="email" class="form-control" name="userName" required placeholder="enter your email address">
      </div>
      <div class="form-group my-4">
        <label for="password">Password</label>
        <input type="password" class="form-control" name="password" required placeholder="enter password">
      </div>
      <div class="form-group mt-5">
        <button type="submit" class="form-control btn btn-dark">Login</button>
      </div>
    </div>  
  </form>
</div>

<%- include('partials/footer') %>